{
  "name": "packages",
  "version": "1.0.0",
  "description": "Show packages info",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node ./build/server/",
    "server": "nodemon ./build/server/",
    "client": "yarn run start --prefix src/client",
    "dev": "concurrently \"yarn run server\" \"yarn run client\"",
    "tsc": "tsc",
    "typescript-watch": "yarn run tsc -- --watch",
    "tslint": "tslint -c tslint.json 'src/server/*.ts' 'src/client/src/*.ts*'",
    "postinstall": "cd src/client && yarn"
  },
  "author": "Ivan",
  "license": "ISC",
  "dependencies": {
    "@types/es6-shim": "^0.31.39",
    "@types/express": "^4.16.0",
    "@types/node": "^10.12.15",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-router-dom": "^4.3.1",
    "client": "file:src/client",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "ramda": "latest",
    "typescript": "^3.2.2"
  }
}
